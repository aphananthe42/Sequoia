[core]
	excludesfile = /Users/Aphananthe/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
[mergetool "sourcetree"]
	cmd = /Applications/Sourcetree.app/Contents/Resources/opendiff-w.sh \"$LOCAL\" \"$REMOTE\" -ancestor \"$BASE\" -merge \"$MERGED\"
	trustExitCode = true
[commit]
	template = /Users/Aphananthe/.stCommitMsg
[init]
	defaultBranch = main
[color]
	ui = auto
[include]
    	path = ~/.gitconfig.local
